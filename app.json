{
  "expo": {
    "name": "Cards",
    "slug": "cards",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.growdev.cards",
      "supportsTablet": true
    },
    "android": {
      "package": "com.growdev.cards"
    }
  }
}
